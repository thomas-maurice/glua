{
  "runtime": {
    "version": "Lua 5.1"
  },
  "workspace": {
    "library": [
      "../library"
    ],
    "checkThirdParty": false
  },
  "diagnostics": {
    "globals": [
      "myPod",
      "originalPod",
      "modifiedPod",
      "exportedReport",
      "exportedPodData",
      "parsedCPUMillis",
      "parsedMemoryBytes",
      "parsedTimestamp"
    ],
    "disable": [
      "duplicate-doc-field"
    ]
  }
}
